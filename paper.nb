Cell[TextData[{
 "Where ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],ExpressionUUID->
  "c28f60c1-63bd-4779-a03e-f8c4e4341ae2"],
 " is percent of memorization while",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", "t", " "}], TraditionalForm]],ExpressionUUID->
  "99e70ee1-53f9-45c4-bed2-688540a2ccdf"],
 "is time and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
